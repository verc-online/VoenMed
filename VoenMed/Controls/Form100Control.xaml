                <!-- Сознание -->
                <StackPanel Orientation="Vertical">
                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="0 0 10 0">Сознание</TextBlock>
                    <StackPanel x:Name="consciencePanel" Orientation="Horizontal">
                        <RadioButton GroupName="Conscience" Content="Ясное" IsChecked="True"
                                     Margin="0 0 10 0" />
                        <RadioButton GroupName="Conscience" Content="Угнетено" Margin="0 0 10 0" />
                        <RadioButton GroupName="Conscience" Content="Отсутствует" Margin="0 0 10 0" />
                        <RadioButton GroupName="Conscience" Content="Кома" />
                    </StackPanel>
                </StackPanel>

                <!-- Частота дыхания -->
                <StackPanel Orientation="Vertical">
                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="0 0 10 0">Частота дыхания</TextBlock>
                    <StackPanel x:Name="respiratoryRatePanel" Orientation="Horizontal">
                        <RadioButton GroupName="RespiratoryRate" Content="больше 94%" Margin="0 0 10 0"
                                     IsChecked="True" />
                        <RadioButton GroupName="RespiratoryRate" Content="меньше 94%" />
                    </StackPanel>
                </StackPanel>

                <!-- Сатурация -->
                <StackPanel Orientation="Vertical">
                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="0 0 10 0">Сатурация</TextBlock>
                    <StackPanel x:Name="saturationPanel" Orientation="Horizontal">
                        <RadioButton GroupName="Saturation" Content="больше 94%" Margin="0 0 10 0"
                                     IsChecked="True" />
                        <RadioButton GroupName="Saturation" Content="меньше 94%" />
                    </StackPanel>
                </StackPanel>

                <!-- Время капил. напол. ногтевого ложа -->
                <StackPanel Orientation="Vertical">
                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="0 0 10 0">Время наполнения ногтевого ложа</TextBlock>
                    <StackPanel x:Name="capillaryTimePanel" Orientation="Horizontal">
                        <RadioButton GroupName="CapillaryTime" Content="Меньше 2 с." Margin="0 0 10 0"
                                     IsChecked="True" />
                        <RadioButton GroupName="CapillaryTime" Content="Больше 2 с." />
                    </StackPanel>
                </StackPanel>

                <!-- Пульс -->
                <StackPanel Orientation="Vertical">
                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="0 0 10 0">Пульс</TextBlock>
                    <StackPanel x:Name="pulsePanel" Orientation="Horizontal">
                        <RadioButton GroupName="Pulse" Content="60-90" IsChecked="True" Margin="0 0 10 0" />
                        <RadioButton GroupName="Pulse" Content="Меньше 60 / Больше 90" />
                    </StackPanel>
                </StackPanel>

                <!-- САД -->
                <StackPanel Orientation="Vertical">
                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="0 0 10 0">Систолическое АД лежа</TextBlock>
                    <StackPanel x:Name="sAdPanel" Orientation="Horizontal">
                        <RadioButton GroupName="sAd" Content="100-160" IsChecked="True" Margin="0 0 10 0" />
                        <RadioButton GroupName="sAd" Content="Меньше 100 / Больше 160" Margin="0 0 10 0" />
                    </StackPanel>
                </StackPanel>

                <!-- Температура тела -->
                <StackPanel Orientation="Vertical">
                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="0 0 0 0">Температура</TextBlock>
                    <StackPanel x:Name="bodyTemperaturePanel" Orientation="Horizontal">
                        <RadioButton GroupName="Temperature" Content="36,0-38,0" Margin="0 0 10 0"
                                     IsChecked="True" />
                        <RadioButton GroupName="Temperature" Content="Меньше 36,0 / Больше 38,0"
                                     Margin="0 0 10 0" />
                    </StackPanel>

                    <!-- Жалобы-->
                    <StackPanel Orientation="Vertical" Grid.ColumnSpan="2">
                        <TextBlock HorizontalAlignment="Center" FontWeight="Bold">Жалобы</TextBlock>
                        <TextBox Margin="5" TextWrapping="Wrap" AcceptsReturn="True" Height="45"></TextBox>
                    </StackPanel>
                </StackPanel>

                <StackPanel Orientation="Vertical">
                    <!-- Дополнительные сведения -->
                    <TextBlock TextWrapping="Wrap" FontWeight="Bold" TextAlignment="Center">
                        Дополнительные сведения
                    </TextBlock>
                    <StackPanel Orientation="Horizontal">
                        <StackPanel Orientation="Vertical" Margin="0 0 0 10">
                            <CheckBox Margin="0 0 10 0">Судороги</CheckBox>
                            <CheckBox Margin="0 0 10 0">Психомоторное возбуждение</CheckBox>
                            <CheckBox Margin="0 0 10 0">Галюцинации</CheckBox>
                        </StackPanel>
                        <StackPanel>
                            <CheckBox Margin="0 0 10 0">Рвота</CheckBox>
                            <CheckBox Margin="0 0 10 0">Диарея</CheckBox>
                            <CheckBox Margin="0 0 10 0">Диарея</CheckBox>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>
            </StackPanel>


        </StackPanel>


    </Grid>
</UserControl>